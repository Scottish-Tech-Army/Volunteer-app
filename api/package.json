{
  "name": "volunteer-app-api",
  "version": "1.0.0",
  "description": "RESTful API to surface the data required for the Volunteer App.",
  "main": "app.js",
  "scripts": {
    "start": "nodemon server.js",
    "tunnel-linux": "export $(cat .env | xargs) && npx localtunnel  --port 3000 --subdomain $API_TUNNEL_SUBDOMAIN",
    "tunnel-windows": "(for /F \"tokens=*\" %i in (.env) do set %i) && npx localtunnel  --port 3000 --subdomain $API_TUNNEL_SUBDOMAIN",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "airtable": "^0.11.1",
    "axios": "^0.25.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "i": "^0.3.7",
    "node-cron": "^3.0.0",
    "node-fetch": "^3.0.0",
    "nodemon": "^2.0.12",
    "vimeo": "^2.1.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-alpha.6",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^27.2.4",
    "nock": "^13.2.0",
    "superagent": "^6.1.0",
    "supertest": "^6.1.6"
  }
}
