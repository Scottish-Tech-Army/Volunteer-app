{
  "cli": {
    "version": ">= 3.13.3"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {}
  },
  "submit": {
    "preview": {
      "android": {
        "serviceAccountKeyPath": "./deployment/google-service-account-key.json",
        "track": "internal"
      }
    },
    "production": {
      "android": {
        "serviceAccountKeyPath": "./deployment/google-service-account-key.json",
        "track": "production"
      }
    }
  }
}
