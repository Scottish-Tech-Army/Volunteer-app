{
  "name": "STA Volunteer app",
  "version": "1.0.11",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "lint-ci": "eslint . -c .eslintrc.ci.js --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc",
    "prepare": "cd .. && husky install .husky",
    "lint-app": "lint-staged --quiet"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.7",
    "@react-native/normalize-color": "^2.1.0",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/stack": "^6.x",
    "@reduxjs/toolkit": "^1.6.1",
    "dayjs": "^1.11.1",
    "fuse.js": "^6.6.2",
    "i18next": "^20.6.1",
    "moment": "^2.29.3",
    "native-base": "^3.4.21",
    "react": "^17.0.2",
    "react-i18next": "^11.12.0",
    "react-native": "0.65.1",
    "react-native-calendar-events": "^2.2.0",
    "react-native-calendar-picker": "^7.1.2",
    "react-native-date-picker": "^4.2.1",
    "react-native-flipper": "^0.113.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.7.2",
    "react-native-simple-markdown": "^1.1.0",
    "react-native-svg": "^12.5.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-swiper-flatlist": "^3.0.16",
    "react-native-vector-icons": "^9.0.0",
    "react-native-view-pdf": "^0.14.0",
    "react-native-webview": "^11.23.1",
    "react-redux": "^7.2.5",
    "redux": "^4.1.1",
    "redux-flipper": "^2.0.1",
    "redux-persist": "^6.0.0",
    "remove-markdown": "^0.5.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@react-native-community/eslint-config": "^3.0.0",
    "@types/react": "^17.0.34",
    "@types/react-native": "^0.66.4",
    "@types/react-native-vector-icons": "^6.4.10",
    "@types/react-redux": "^7.1.20",
    "@types/remove-markdown": "^0.3.1",
    "@types/styled-components-react-native": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^8.x",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-jest": "^26.7.0",
    "eslint-plugin-jsdoc": "^39.3.4",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react-native-codegen": "^0.0.7",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./jest.setup.js"
    ],
    "timers": "fake",
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "/node_modules/(?!@react-native|react-native)"
    ]
  },
  "lint-staged": {
    "*.{js,ts,tsx, jsx}": [
      "eslint --fix"
    ],
    "*.{json,md,html,js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  }
}
